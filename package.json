    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "ee": "git://github.com/dr4fters/ee.git",
    "engine.io": "^3.3.2",
    "engine.io-client": "^3.3.1",
    "html-webpack-plugin": "3.2.0",
    "jsonfile": "^4.0.0",
    "log-timestamp": "^0.2.1",
    "node-fetch": "^2.3.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "send": "^0.16.2",
